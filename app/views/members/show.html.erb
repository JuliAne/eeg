<p>
  <b>Lastname:</b>
  <%=h @member.lastname %>
</p>

<p>
  <b>Firstname:</b>
  <%=h @member.firstname %>
</p>

<p>
  <b>Information:</b>
  <%=h @member.information %>
</p>

<p>
	<b>Position:</b>
	<%= h @member.position %>
</p>

<p>
<b>Elements:</b>
  <% @member.elements.each do |e| %>
    <%= render_element(e) %>
  <% end%>
</p>

<%= link_to 'Edit', edit_member_path(@member) %> |
<%= link_to 'Back', members_path %>
