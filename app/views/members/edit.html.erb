<% content_for :background, @member.background.data.url if @member.background %>

<div class="admin">
<h1>Editing member</h1>

<% form_for @member, :html => { :multipart => true } do |f| %>
  <%= render :partial => 'formfields', :locals => { :f => f } %>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @member %> |
<%= link_to 'Back', members_path %>
</div>
